  <tr>
    <td><%=h house_menu.title %></td>
    <td><%=h house_menu.link %></td>
    <td><%= link_to 'Show', house_menu %></td>
    <td><%= link_to 'Edit', edit_house_menu_path(house_menu) %></td>
    <td><%= link_to 'Destroy', house_menu, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

